{
    "name": "tkaratug/laravel-notification-event-subscriber",
    "description": "This is my package laravel-notification-event-subscriber",
    "keywords": [
        "tkaratug",
        "laravel",
        "laravel-notification-event-subscriber"
    ],
    "homepage": "https://github.com/tkaratug/laravel-notification-event-subscriber",
    "license": "MIT",
    "authors": [
        {
            "name": "Turan KaratuÄŸ",
            "email": "tkaratug@hotmail.com.tr",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/contracts": "^9.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "orchestra/testbench": "^7.0",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Tkaratug\\NotificationEventSubscriber\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tkaratug\\NotificationEventSubscriber\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tkaratug\\NotificationEventSubscriber\\NotificationEventSubscriberServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
